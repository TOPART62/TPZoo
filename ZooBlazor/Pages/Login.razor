@page "/Login"
@using ZooBlazor.Services;
@using ZooBlazor.DTOs

<h3>Login</h3>
<form method="post" asp-page="Index">
    <table>
        <tr>
            <td>User</td>
            <td><input type="text" asp-for="User" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="password" asp-for="Password" /></td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" value="Login" /></td>
        </tr>
    </table>
</form>
